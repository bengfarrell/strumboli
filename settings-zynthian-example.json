{
  "startupConfiguration": {
    "midiOutputBackend": "jack",
    "jackClientName": "midi_strummer",
    "drawingTablet": "auto-detect",
    "useSocketServer": true,
    "socketServerPort": 8080,
    "midiInputId": null
